# Streamlit Secrets Template
# Copy this file to secrets.toml and fill in your actual values

[general]
TEST_MODE = false
OPENAI_API_KEY = "your_openai_api_key_here"

[database]
SUPABASE_URL = "your_supabase_url_here"
SUPABASE_KEY = "your_supabase_anon_key_here"

# Instructions:
# 1. Copy this file: cp .streamlit/secrets.toml.example .streamlit/secrets.toml
# 2. Edit .streamlit/secrets.toml with your actual API keys
# 3. For Streamlit Cloud, use the same TOML structure in Advanced Settings > Secrets
# 4. See README.md for detailed setup instructions